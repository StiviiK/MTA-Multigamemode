function sizeFormat(size)
	local size = tostring(size)
	if size:len() >= 4 then
		if size:len() >= 7 then
			if size:len() >= 9 then
				local returning = tostring(size/(1024*1024*1024))
				returning = returning:sub(1, 4)
				return returning.."
